<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="com.exercici0601.ControllerItem">
    <Circle fx:id="circleColor" visible="false" managed="false" radius="5" />
    <ImageView fx:id="imgCharacter" fitWidth="50" fitHeight="50" />
    <VBox>
        <Label fx:id="lblName" style="-fx-font-weight: bold;"/>
        <Label fx:id="lblGame" style="-fx-font-size: 12px;"/>
    </VBox>
</HBox>