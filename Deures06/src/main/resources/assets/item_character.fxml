<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<HBox alignment="CENTER_LEFT" spacing="10" style="-fx-border-color: lightgray; -fx-padding: 10;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fx:id="imgCharacter" fitHeight="50" fitWidth="50">
      <HBox.margin>
         <Insets right="10.0" />
      </HBox.margin></ImageView>
    <VBox spacing="5">
        <Label fx:id="lblName" style="-fx-font-weight: bold; -fx-font-size: 14px;" />
        <Label fx:id="lblGame" style="-fx-font-size: 12px; -fx-text-fill: #555;" />
    </VBox>
    <Circle fx:id="circleColor" radius="10" />
</HBox>
